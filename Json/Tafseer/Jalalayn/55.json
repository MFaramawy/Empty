[
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/1/",
        "ayah_number": 1,
        "text": "«الرحمن» الله تعالى."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/2/",
        "ayah_number": 2,
        "text": "«علَّم» من شاء «القرآن»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/3/",
        "ayah_number": 3,
        "text": "«خلق الإنسان» أي الجنس."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/4/",
        "ayah_number": 4,
        "text": "«علَّمه البيان» النطق."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/5/",
        "ayah_number": 5,
        "text": "«الشمس والقمر بحسبان» يجريان."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/6/",
        "ayah_number": 6,
        "text": "«والنجم» ما لا ساق له من النبات «والشجر» ما له ساق «يسجدان» يخضعان لما يراد منهما."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/7/",
        "ayah_number": 7,
        "text": "«والسماء رفعها ووضع الميزان» أثبت العدل."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/8/",
        "ayah_number": 8,
        "text": "«ألا تطغوا» أي لأجل أن لا تجوروا «في الميزان» ما يوزن به."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/9/",
        "ayah_number": 9,
        "text": "«وأقيموا الوزن بالقسط» بالعدل «ولا تخسروا الميزان» تنقصوا الموزون."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/10/",
        "ayah_number": 10,
        "text": "«والأرض وضعها» أثبتها «للأنام» للخلق الإنس والجن وغيرهم."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/11/",
        "ayah_number": 11,
        "text": "«فيها فاكهة والنخل» المعهود «ذات الأكمام» أوعية طلعها."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/12/",
        "ayah_number": 12,
        "text": "«والحب» كالحنطة والشعير «ذو العصف» التين «والريحان» الورق المشموم."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/13/",
        "ayah_number": 13,
        "text": "(فبأي آلاء) نعم (ربكما) أيها الإنس والجن (تكذبان) ذكرت إحدى وثلاثين مرة، والاستفهام فيها للتقرير لما روي الحاكم عن جابر قال: \"\" قرأ علينا رسول الله صلى الله عليه وسلم سورة الرحمن حتى ختمها، ثم قال: مالي أراكم سكوتا، لَلْجن كانوا أحسن منكم ردا ما قرأت عليهم هذه الآية من مرة (فبأي آلاء ربكما تكذبان) إلا قالوا: ولا بشيء من نعمك ربنا نكذب فلك الحمد \"\"."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/14/",
        "ayah_number": 14,
        "text": "«خلق الإنسان» آدم «من صلصال» طين يابس يسمع له صلصلة، أي صوت إذا نقر «كالفخار» وهو ما طبخ من الطين."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/15/",
        "ayah_number": 15,
        "text": "«وخلق الجان» أبا الجن وهو إبليس «من مارج من نار» هو لهبها الخالص من الدخان."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/16/",
        "ayah_number": 16,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/17/",
        "ayah_number": 17,
        "text": "«رب المشرقين» مشرق الشتاء ومشرق الصيف «ورب المغربين» كذلك."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/18/",
        "ayah_number": 18,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/19/",
        "ayah_number": 19,
        "text": "«مرج» أرسل «البحرين» العذب والملح «يلتقيان» في رأي العين."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/20/",
        "ayah_number": 20,
        "text": "«بينهما برزخ» حاجز من قدرته تعالى «لا يبغيان» لا يبغي واحد منهما على الآخر فيختلط به."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/21/",
        "ayah_number": 21,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/22/",
        "ayah_number": 22,
        "text": "«يَُخرج» بالبناء للمفعول والفاعل «منهما» من مجموعهما الصادق بأحدهما وهو الملح «اللؤلؤ المرجان» خرز أحمر أو صغار اللؤلؤ."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/23/",
        "ayah_number": 23,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/24/",
        "ayah_number": 24,
        "text": "«وله الجوار» السفن «المنشآت» المحدثات «في البحر كالأعلام» كالجبال عظما وارتفاعا."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/25/",
        "ayah_number": 25,
        "text": "«فبأي آلاء تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/26/",
        "ayah_number": 26,
        "text": "«كل من عليها» أي الأرض من الحيوان «فان» هالك وعبر بمن تغليبا للعقلاء."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/27/",
        "ayah_number": 27,
        "text": "«ويبقى وجه ربك» ذاته «ذو الجلال» العظمة «والإكرام» للمؤمنين بأنعمه عليهم."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/28/",
        "ayah_number": 28,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/29/",
        "ayah_number": 29,
        "text": "«يسأله من في السماوات والأرض» بنطق أو حال: ما يحتاجون إليه من القوة على العبادة والرزق والمغفرة وغير ذلك «كل يوم» وقت «هو في شأن» أمر يُظهره على وفق ما قدره في الأزل من إحياء و إماتة وإعزاز وإذلال وإغناء وإعدام وإجابة داع وإعطاء سائل وغير ذلك."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/30/",
        "ayah_number": 30,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/31/",
        "ayah_number": 31,
        "text": "«سنفرغ لكم» سنقصد لحسابكم «أيها الثقلان» الإنس والجن."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/32/",
        "ayah_number": 32,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/33/",
        "ayah_number": 33,
        "text": "«يا معشر الجن والإنس إن استطعتم أن تنفذوا» تخرجوا «من أقطار» نواحي «السماوات والأرض فانفذوا» أمر تعجيز «لا تنفذون إلا بسلطان» بقوة ولا قوة لكم على ذلك."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/34/",
        "ayah_number": 34,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/35/",
        "ayah_number": 35,
        "text": "«يرسل عليكما شواظ من نار» هو لهبها الخالص من الدخان أو معه «ونحاس» أي دخان لا لهب فيه «فلا تنتصران» تمتنعان من ذلك بل يسوقكم إلى المحشر."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/36/",
        "ayah_number": 36,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/37/",
        "ayah_number": 37,
        "text": "«فإذا انشقت السماء» انفرجت أبوابا لنزول الملائكة «فكانت وردة» أي مثلها محمرة «كالدهان» كالأديم الأحمر على خلاف العهد بها وجواب إذا فما أعظم الهول."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/38/",
        "ayah_number": 38,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/39/",
        "ayah_number": 39,
        "text": "«فيومئذ لا يُسأل عن ذنبه إنس ولا جان» عن ذنبه ويُسألون في وقت آخر (فوربك لنسألنَّهم أجمعين) والجان هنا وفيما سيأتي بمعنى الجن والإنس فيهما بمعنى الإنسي."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/40/",
        "ayah_number": 40,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/41/",
        "ayah_number": 41,
        "text": "«يعرف المجرمون بسيماهم» سواد الوجوه وزرقة العيون «فيؤخذ بالنواصي الأقدام»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/42/",
        "ayah_number": 42,
        "text": "«فبأي آلاء ربكما تكذبان» تضم ناصية كل منهم إلى قدميه من خلف أو قدام ويلقى في النار ويقال لهم."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/43/",
        "ayah_number": 43,
        "text": "«هذه جهنم التي يكذب بها المجرمون»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/44/",
        "ayah_number": 44,
        "text": "«يطوفون» يسعون «بينها وبين حميم» ماء حار «آن» شديد الحرارة يسقونه إذا استغاثوا من حر النار، وهو منقوص كقاض."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/45/",
        "ayah_number": 45,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/46/",
        "ayah_number": 46,
        "text": "«ولمن خاف» أي لكل منهم أو لمجموعهم «مقام ربه» قيامه بين يديه للحساب فترك معصيته «جنتان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/47/",
        "ayah_number": 47,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/48/",
        "ayah_number": 48,
        "text": "«ذواتا» تثنية ذوات على الأصل ولامها ياء «أفنان» أغصان جمع فنن كطلل."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/49/",
        "ayah_number": 49,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/50/",
        "ayah_number": 50,
        "text": "«فيهما عينان تجريان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/51/",
        "ayah_number": 51,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/52/",
        "ayah_number": 52,
        "text": "«فيهما من كل فاكهة» في الدنيا أو كل ما يتفكه به «زوجان» نوعان رطب ويابس والمر منهما في الدنيا كالحنظل حلو."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/53/",
        "ayah_number": 53,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/54/",
        "ayah_number": 54,
        "text": "«متكئين» حال عامله محذوف، أي يتنعمون «على فرش بطائنها من إستبرق» ما غلظ من الديباج وخشن والظهائر من السندس «وجنى الجنتين» ثمرهما «دان» قريب يناله القائم والقاعد والمضطجع."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/55/",
        "ayah_number": 55,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/56/",
        "ayah_number": 56,
        "text": "«فيهن» في الجنتين وما اشتملتا عليه من العلالي والقصور «قاصرات الطرف» العين على أزواجهن المتكئين من الإنس والجن «لم يطمثهن» يفتضهن وهن من الحور أو من نساء الدنيا المنشآت «إنس قبلهم ولا جان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/57/",
        "ayah_number": 57,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/58/",
        "ayah_number": 58,
        "text": "«كأنهن الياقوت» صفاء «والمرجان» اللؤلؤ بياضا."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/59/",
        "ayah_number": 59,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/60/",
        "ayah_number": 60,
        "text": "«هل» ما «جزاء الإحسان» بالطاعة «إلا الإحسان» بالنعيم."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/61/",
        "ayah_number": 61,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/62/",
        "ayah_number": 62,
        "text": "«ومن دونهما» الجنتين المذكورتين «جنتان» أيضا لمن خاف مقام ربه."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/55/63/",
        "ayah_number": 63,
        "text": "«فبأي آلاء ربكما تكذبان»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
