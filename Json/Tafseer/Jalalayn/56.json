[
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/1/",
        "ayah_number": 1,
        "text": "«إذا وقعت الواقعة» قامت القيامة."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/2/",
        "ayah_number": 2,
        "text": "«ليس لوقعتها كاذبة» نفس تكذب بأن تنفيها كما نفتها في الدنيا."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/3/",
        "ayah_number": 3,
        "text": "«خافضة رافعة» أي هي مظهرة لخفض أقوام بدخولهم النار ولرفع آخرين بدخولهم الجنة."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/4/",
        "ayah_number": 4,
        "text": "«إذا رجت الأرض رجا» حركت حركة شديدة."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/5/",
        "ayah_number": 5,
        "text": "«وبست الجبال بسا» فتتت."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/6/",
        "ayah_number": 6,
        "text": "«فكانت هباءً» غبارا «منبثا» منتشرا، وإذا الثانية بدل من الأولى."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/7/",
        "ayah_number": 7,
        "text": "«وكنتم» في القيامة «أزواجا» أصنافا «ثلاثة»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/8/",
        "ayah_number": 8,
        "text": "«فأصحاب الميمنة» وهم الذين يؤتون كتبهم بإيمانهم مبتدأ خبره «ما أصحاب الميمنة» تعظيم لشأنهم بدخولهم الجنة."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/9/",
        "ayah_number": 9,
        "text": "«وأصحاب المشأمة» أي الشمال بأن يؤتى كل منهم كتابه بشماله «ما أصحاب المشأمة» تحقير لشأنهم بدخولهم النار."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/10/",
        "ayah_number": 10,
        "text": "«والسابقون» إلى الخير وهم الأنبياء مبتدأ «السابقون» تأكيد لتعظيم شأنهم."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/11/",
        "ayah_number": 11,
        "text": "«أولئك المقربون»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/12/",
        "ayah_number": 12,
        "text": "«في جنات النعيم»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/13/",
        "ayah_number": 13,
        "text": "«ثلة من الأولين» مبتدأ، أي جماعة من الأمم الماضية."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/14/",
        "ayah_number": 14,
        "text": "«وقليل من الآخرين» من أمة محمد صلى الله عليه وسلم وهم السابقون من الأمم الماضية وهذه الأمة والخبر."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/15/",
        "ayah_number": 15,
        "text": "«على سرر موضونة» منسوجة بقضبان الذهب والجواهر."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/16/",
        "ayah_number": 16,
        "text": "«متكئين عليها متقابلين» حالان من الضمير في الخبر."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/17/",
        "ayah_number": 17,
        "text": "«يطوف عليهم» للخدمة «ولدان مخلدون» على شكل الأولاد لا يهرمون."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/18/",
        "ayah_number": 18,
        "text": "«بأكواب» أقداح لا عرى لها «وأباريق» لها عرى وخراطيم «وكأس» إناء شرب الخمر «من معين» أي خمر جارية من منبع لا ينقطع أبدا."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/19/",
        "ayah_number": 19,
        "text": "«لا يصدعون عنها ولا ينزَِفون» بفتح الزاي وكسرها من نزف الشارب وأنزف، أي لا يحصل لهم منها صداع ولا ذهاب عقل بخلاف خمر الدنيا."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/20/",
        "ayah_number": 20,
        "text": "«وفاكهة مما يتخيرون»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/21/",
        "ayah_number": 21,
        "text": "«ولحم طير مما يشتهون و» لهم للاستمتاع."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/22/",
        "ayah_number": 22,
        "text": "«وحور» نساء شديدات سواد العيون وبياضها «عين» ضخام العيون كسرت عينه بدل ضمها لمجانسة الياء ومفرده عيناء كحمراء وفي قراءة بجر حور عين."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/23/",
        "ayah_number": 23,
        "text": "«كأمثال اللؤلؤ المكنون» المصون."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/24/",
        "ayah_number": 24,
        "text": "«جزاءً» مفعول له أو مصدر والعامل المقدر أي جعلنا لهم ما ذكر للجزاء أو جزيناهم «بما كانوا يعملون»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/25/",
        "ayah_number": 25,
        "text": "«لا يسمعون فيها» في الجنة «لغوا» فاحشا من الكلام «ولا تأثيما» ما يؤثم."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/26/",
        "ayah_number": 26,
        "text": "«إلا» لكن «قيلا» قولا «سلاما» سلاما بدل من قيلا فإنهم يسمعونه."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/27/",
        "ayah_number": 27,
        "text": "«وأصحاب اليمين ما أصحاب اليمين»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/28/",
        "ayah_number": 28,
        "text": "«في سدر» شجر النبق «مخضود» لا شوك فيه."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/29/",
        "ayah_number": 29,
        "text": "«وطلح» شجر الموز «منضود» بالحمل من أسفله إلى أعلاه."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/30/",
        "ayah_number": 30,
        "text": "«وظل ممدود» دائم."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/31/",
        "ayah_number": 31,
        "text": "«وماء مسكوب» جار دائما."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/32/",
        "ayah_number": 32,
        "text": "«وفاكهة كثيرة»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/33/",
        "ayah_number": 33,
        "text": "«لا مقطوعة» في زمن «ولا ممنوعة» بثمن."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/34/",
        "ayah_number": 34,
        "text": "«وفرش مرفوعة» على السرر."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/35/",
        "ayah_number": 35,
        "text": "«إنا أنشأناهن إنشاءً» أي الحور العين من غير ولادة."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/36/",
        "ayah_number": 36,
        "text": "«فجعلناهن أبكارا» عذارى كلما أتاهن أزواجهن وجدوهن عذارى ولا وجع."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/37/",
        "ayah_number": 37,
        "text": "«عرُْبا» بضم الراء وسكونها جمع عروب وهي المتحببة إلى زوجها عشقا له «أترابا» جمع ترب، أي مستويات في السن."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/38/",
        "ayah_number": 38,
        "text": "«لأصحاب اليمين» صلة أنشأناهن أو جعلناهن وهم."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/39/",
        "ayah_number": 39,
        "text": "«ثلة من الأولين»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/40/",
        "ayah_number": 40,
        "text": "«وثلة من الآخرين»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/41/",
        "ayah_number": 41,
        "text": "«وأصحاب الشمال ما أصحاب الشمال»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/42/",
        "ayah_number": 42,
        "text": "«في سموم» ريح جارة من النار تنفذ في المسام «وحميم» ماء شديد الحرارة."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/43/",
        "ayah_number": 43,
        "text": "«وظل من يحموم» دخان شديد السواد."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/44/",
        "ayah_number": 44,
        "text": "«لا بارد» كغيره من الظلال «ولا كريم» حسن المنظر."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/45/",
        "ayah_number": 45,
        "text": "«إنهم كانوا قبل ذلك» في الدنيا «مترفين» منعمين لا يتعبون في الطاعة."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/46/",
        "ayah_number": 46,
        "text": "«وكانوا يصرون على الحنث» الذنب «العظيم» أي الشرك."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/47/",
        "ayah_number": 47,
        "text": "«وكانوا يقولون أئذا متنا وكنا ترابا وعظاما أئنا لمبعوثون» في الهمزتين في الموضوعين التحقيق وتسهيل الثانية وإدخال ألف بينهما على الوجهين."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/48/",
        "ayah_number": 48,
        "text": "«أو آباؤنا الأولون» بفتح الواو للعطف والهمزة للاستفهام وهو في ذلك وفيما قبله للاستبعاد وفي قراءة بسكون الواو عطفا بأو والمعطوف عليه محل إن واسمها."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/49/",
        "ayah_number": 49,
        "text": "«قل إن الأولين والآخرين»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/50/",
        "ayah_number": 50,
        "text": "«لمجموعون إلى ميقات» لوقت «يوم معلوم» أي يوم القيامة."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/51/",
        "ayah_number": 51,
        "text": "«ثم إنكم أيها الضالون المكذبون»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/52/",
        "ayah_number": 52,
        "text": "«لآكلون من شجر من زقوم» بيان للشجر."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/53/",
        "ayah_number": 53,
        "text": "«فمالئون منها» من الشجر «البطون»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/54/",
        "ayah_number": 54,
        "text": "«فشاربون عليه» أي الزقوم المأكول «من الحميم»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/55/",
        "ayah_number": 55,
        "text": "«فشاربون شَُرب» بفتح الشين وضمها مصدر «الهيم» الإبل العطاش جمع هيمان للذكر وهيمى للأنثى، كعطشان وعطشى."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/56/",
        "ayah_number": 56,
        "text": "«هذا نزلهم» ما أعد لهم «يوم الدين» يوم القيامة."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/57/",
        "ayah_number": 57,
        "text": "«نحن خلقناكم» أوجدناكم من عدم «فلولا» هلا «تصدقون» بالبعث إذ القادر على الإنشاء قادر على الإعادة."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/58/",
        "ayah_number": 58,
        "text": "«أفرأيتم ما تمنون» تريقون من المني في أرحام النساء."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/59/",
        "ayah_number": 59,
        "text": "«أأنتم» بتحقيق الهمزتين وإبدال الثانية ألفا وتسهيلها وإدخال ألف بين المسهلة والأخرى وتركه في المواضع الأخرى «تخلقونه» أي المني بشرا «أم نحن الخالقون»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/60/",
        "ayah_number": 60,
        "text": "«نحن قدَّرنا» بالتشديد والتخفيف «بينكم الموت وما نحن بمسبوقين» بعاجزين."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/61/",
        "ayah_number": 61,
        "text": "«على» عن «أن نبدل» نجعل «أمثالكم» مكانكم «وننشئكم» نخلقكم «في مالا تعلمون» من الصور كالقردة والخنازير."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/62/",
        "ayah_number": 62,
        "text": "«ولقد علمتم النّشاءَةَ الأولى» وفي قراءة بسكون الشين «فلولا تذكرون» فيه إدغام التاء الثانية في الأصل في الذال."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/63/",
        "ayah_number": 63,
        "text": "«أفرأيتم ما تحرثون» تثيرون في الأرض وتلقون البذر فيها."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/64/",
        "ayah_number": 64,
        "text": "«أأنتم تزرعونه» تنبتونه «أم نحن الزارعون»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/65/",
        "ayah_number": 65,
        "text": "«لو نشاء لجعلناه حطاما» نباتا يابسا لا حب فيه «فظلتم» أصله ظللتم بكسر اللام حذفت تخفيفا أي أقمتم نهارا «تفكهون» حذفت منه إحدى التاءين في الأصل تعجبون من ذلك وتقولون."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/66/",
        "ayah_number": 66,
        "text": "«إنا لمغرمون» نفقة زرعنا."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/67/",
        "ayah_number": 67,
        "text": "«بل نحن محرومون» ممنوعون رزقنا."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/68/",
        "ayah_number": 68,
        "text": "«أفرأيتم الماء الذي تشربون»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/69/",
        "ayah_number": 69,
        "text": "«أأنتم أنزلتموه من المزن» السحاب جمع مزنة «أم نحن المنزلون»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/70/",
        "ayah_number": 70,
        "text": "«لو نشاء جعلناه أجاجا» ملحا لا يمكن شربه «فلولا» هلا «تشكرون»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/71/",
        "ayah_number": 71,
        "text": "«أفرأيتم النار التي تورون» تُخرجون من الشجر الأخضر."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/72/",
        "ayah_number": 72,
        "text": "«أأنتم أنشأتم شجرتها» كالمرخ والعفار والكلخ «أم نحن المنشئون»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/73/",
        "ayah_number": 73,
        "text": "«نحن جعلناها تذكرة» لنار جهنم «ومتاعا» بُلْغَة «للمقوين» للمسافرين من أقوى القوم: أي صاروا بالقوى بالقصر والمد أي القفر وهو مفازة لا نبات فيها ولا ماء."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/74/",
        "ayah_number": 74,
        "text": "«فسبح» نزه «باسم» زائدة «ربك العظيم» الله."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/75/",
        "ayah_number": 75,
        "text": "«فلا أقسم» لا زائدة «بمواقع النجوم» بمساقطها لغروبها."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/76/",
        "ayah_number": 76,
        "text": "«وإنه» أي القسم بها «لقسم لو تعلمون عظيم» لو كنتم من ذوي العلم لعلمتم عظم هذا القسم."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/77/",
        "ayah_number": 77,
        "text": "«إنه» أي المتلو عليكم «لقرآن كريم»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/78/",
        "ayah_number": 78,
        "text": "«في كتاب» مكتوب «مكنون» مصون وهو المصحف."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/79/",
        "ayah_number": 79,
        "text": "«لايمسّه» خبر بمعنى النهي «إلا المطهرون» الذين طهروا أنفسهم من الأحداث."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/80/",
        "ayah_number": 80,
        "text": "«تنزيل» منزل «من رب العالمين»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/81/",
        "ayah_number": 81,
        "text": "«أفبهذا الحديث» القرآن «أنتم مدهنون» متهاونون مكذبون."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/82/",
        "ayah_number": 82,
        "text": "«وتجعلون رزقكم» من المطر، أي شكره «أنكم تكذبون» بسقيا الله حيث قلتم مطرنا بنوء كذا."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/83/",
        "ayah_number": 83,
        "text": "«فلولا» فهلا «إذا بلغت» الروح وقت النزع «الحلقوم» هو مجرى الطعام."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/84/",
        "ayah_number": 84,
        "text": "«وأنتم» يا حاضري الميت «حينئذ تنظرون» إليه."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/85/",
        "ayah_number": 85,
        "text": "«ونحن أقرب إليه منكم» بالعلم «ولكن لا تبصرون» من البصيرة، أي لا تعلمون ذلك."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/86/",
        "ayah_number": 86,
        "text": "«فلولا» فهلا «إن كنتم غير مدينين» مجزيين بأن تبعثوا، أي غير مبعوثين بزعمكم."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/87/",
        "ayah_number": 87,
        "text": "«ترجعونها» تردون الروح إلى الجسد بعد بلوغ الحلقوم «إن كنتم صادقين» فيما زعمتم فلولا الثانية تأكيد للأولى وإذا ظرف لترجعون المتعلق به الشرطان والمعنى: هلا ترجعونها إن نفيتم البعث صادقين في نفيه، أي لينتفي عن محلها الموت كالبعث."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/88/",
        "ayah_number": 88,
        "text": "«فأما إن كان» الميت «من المقربين»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/89/",
        "ayah_number": 89,
        "text": "«فروْح» أي فله استراحة «وريحان» رزق حسن «وجنة نعيم» وهل الجواب لأما أو لإن أو لهما؟ أقوال."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/90/",
        "ayah_number": 90,
        "text": "«وأما إن كان من أصحاب اليمين»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/91/",
        "ayah_number": 91,
        "text": "«فسلام لك» أي له السلامة من العذاب «من أصحاب اليمين» من جهة أنه منهم."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/92/",
        "ayah_number": 92,
        "text": "«وأما إن كان من المكذبين الضالين»."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/93/",
        "ayah_number": 93,
        "text": "(فنزل من حميم) من إضافة الموصوف إلى صفته."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/94/",
        "ayah_number": 94,
        "text": "(وتصلية جحيم) من إضافة الموصوف إلى صفته."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/95/",
        "ayah_number": 95,
        "text": "«إن هذا لهو حق اليقين» من إضافة الموصوف إلى صفته."
    },
    {
        "tafseer_id": 2,
        "tafseer_name": "تفسير الجلالين",
        "ayah_url": "/quran/56/96/",
        "ayah_number": 96,
        "text": "«فسبح باسم ربك العظيم» تقدم."
    }
]