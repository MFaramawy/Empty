    {
        "tafseer_id": 9,
        "tafseer_name": "Arberry",
        "ayah_url": "/quran/96/6/",
        "ayah_number": 6,
        "text": "No indeed; surely Man waxes insolent,"
    },
    {
        "tafseer_id": 9,
        "tafseer_name": "Arberry",
        "ayah_url": "/quran/96/7/",
        "ayah_number": 7,
